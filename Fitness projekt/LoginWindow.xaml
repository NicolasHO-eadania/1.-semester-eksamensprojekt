<Window x:Class="Fitness_projekt.LoginWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Fitness_projekt"
        mc:Ignorable="d"
        Title="LoginWindow" Height="225" Width="400">
    <Viewbox x:Name="LoginViewBox" Stretch="Uniform">
        <Grid Height="209" Width="400">
            <TextBox x:Name="BrugernavnTextBox" HorizontalAlignment="Left" Height="25" Margin="180,50,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="150" FontSize="16"/>
            <TextBox x:Name="AdgangskodeTextBox" HorizontalAlignment="Left" Height="25" Margin="180,80,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="150" FontSize="16"/>
            <TextBlock HorizontalAlignment="Left" Height="25" Margin="70,50,0,0" TextWrapping="Wrap" Text="Brugernavn" VerticalAlignment="Top" Width="100" FontSize="16"/>
            <TextBlock HorizontalAlignment="Left" Height="24" Margin="70,80,0,0" TextWrapping="Wrap" Text="Adgangskode" VerticalAlignment="Top" Width="100" FontSize="16"/>
            <Button x:Name="LogIndButton" Content="Log ind" HorizontalAlignment="Center" Height="30" Margin="0,122,0,0" VerticalAlignment="Top" Width="260" FontSize="16" Click="LogIndButton_Click"/>
        </Grid>
    </Viewbox>
</Window>
